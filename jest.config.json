{
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!**/{node_modules,lib,coverage,assets,utils}/**",
    "!**/*.story.{js,jsx}"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "setupFiles": [
    "<rootDir>/util/setupTest.js"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleNameMapper": {
    "\\.(ico|inc|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/emptyString.js"
  },
  "testRegex": "src/.*\\.test\\.jsx?$",
  "transformIgnorePatterns": [
    "node_modules(?!\/meetup-web-components)"
  ],
  "snapshotSerializers": [
    "<rootDir>/node_modules/enzyme-to-json/serializer"
  ]
}
